package com.unionpay.sms.model;

import java.util.Date;

/**
 * 申请发送短信的字段
 *
 * @author wangyue
 * @date 2018-12-7
 */
public class smsMatterCondition {
	/** 申请表unid（由页面返回--即用户进入短信发送平台后，自动生成的unid） */
	private String unid;
	/** 申请标题 */
	private String title;
	/** 申请原因 */
	private String reason;
	/** 短信内容 */
	private String content;
	/** 定时发送时间 */
	private String timingTime;
	/** 短信发送状态 */
	private String status;
	/** 提交时间 */
	private String submitTime;
	/** 提交类型：0送领导审批 1保存	 */
	private Integer Type;
	private String submitStartTime;
	private String submitEndTime;
	private String orgLevel;
	private String mode;
	private String approverId;
	private String approverName;

	public String getUnid() {
		return unid;
	}

	public void setUnid(String unid) {
		this.unid = unid;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getReason() {
		return reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public String getTimingTime() {
		return timingTime;
	}

	public void setTimingTime(String timingTime) {
		this.timingTime = timingTime;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getSubmitTime() {
		return submitTime;
	}

	public void setSubmitTime(String submitTime) {
		this.submitTime = submitTime;
	}

	public Integer getType() {
		return Type;
	}

	public void setType(Integer type) {
		Type = type;
	}

	public String getSubmitStartTime() {
		return submitStartTime;
	}

	public void setSubmitStartTime(String submitStartTime) {
		this.submitStartTime = submitStartTime;
	}

	public String getSubmitEndTime() {
		return submitEndTime;
	}

	public void setSubmitEndTime(String submitEndTime) {
		this.submitEndTime = submitEndTime;
	}

	public String getOrgLevel() {
		return orgLevel;
	}

	public void setOrgLevel(String orgLevel) {
		this.orgLevel = orgLevel;
	}

	public String getMode() {
		return mode;
	}

	public void setMode(String mode) {
		this.mode = mode;
	}

	public String getApproverId() {
		return approverId;
	}

	public void setApproverId(String approverId) {
		this.approverId = approverId;
	}

	public String getApproverName() {
		return approverName;
	}

	public void setApproverName(String approverName) {
		this.approverName = approverName;
	}

	@Override
	public String toString() {
		return "smsMatterCondition{" +
				"unid='" + unid +
				", title='" + title +
				", reason='" + reason +
				", content='" + content +
				", timingTime='" + timingTime +
				", status='" + status +
				", submitTime='" + submitTime +
				", Type=" + Type +
				", submitStartTime='" + submitStartTime +
				", submitEndTime='" + submitEndTime +
				", orgLevel='" + orgLevel +
				", mode='" + mode +
				", approverId='" + approverId +
				", approverName='" + approverName +
				'}';
	}
}
